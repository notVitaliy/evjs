  },
  "homepage": "https://github.com/notVitaliy/evjs#readme",
  "dependencies": {
    "chalk": "^2.0.1",
    "chance": "^1.0.10",
    "ts-node": "^3.1.0",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/sinon": "^2.3.2",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "sinon": "^2.3.6"
