{
  "name": "evjs",
  "version": "0.1.4",
  "description": "Generic genetic/evolution algorithm in JS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "prepublish": "npm run clean && tsc",
    "test": "nyc mocha ./src/**/*.spec.ts",
    "report": "nyc report --reporter=html"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.mock.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notVitaliy/evjs.git"
  },
  "author": "notVitaliy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notVitaliy/evjs/issues"
  },
  "homepage": "https://github.com/notVitaliy/evjs#readme",
  "dependencies": {
    "chalk": "^2.0.1",
    "chance": "^1.0.10",
    "evjs": "^0.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/sinon": "^2.3.2",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.6",
    "ts-node": "^3.1.0",
    "typescript": "^2.4.1"
  }
}
